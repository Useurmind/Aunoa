{
    "compilerOptions": {
        "noImplicitAny": false,
        "noEmitOnError": true,
        "removeComments": false,
        "sourceMap": true,
        "module": "commonjs",
        "moduleResolution": "node",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "target": "es5",
        "outDir": "./wwwroot/"
    },
    "exclude": [
        "node_modules",
        "typings/main",
        "typings/main.d.ts",
        "wwwroot",
        "external"
    ]
}
